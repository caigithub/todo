{
    "name": "d",
    "displayName": "d",
    "description": "use plain txt to mark task status",
    "version": "0.0.14",
    "publisher": "colinkaopu",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
